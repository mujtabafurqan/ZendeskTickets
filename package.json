{
  "name": "zendesk-tickets",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/app.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/app.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/app.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "mocha 'src/server/tests/**/*.js'"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^0.24.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "5.1.3",
    "dotenv": "^10.0.0",
    "express": "^4.16.3",
    "nodejs-base64": "^2.0.0",
    "react": "^16.5.2",
    "react-bootstrap": "^2.0.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^2.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^9.1.3",
    "nock": "^13.2.1",
    "nodemon": "^1.17.3",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  }
}
